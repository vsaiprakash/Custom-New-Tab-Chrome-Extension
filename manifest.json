{
  "manifest_version": 2,

  "name": "Startup Page",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "chrome_url_overrides" : {
    "newtab": "startup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  
  "permissions": [
          "storage"
  ],
  
  "options_page": "settings.html"
  
}